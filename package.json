{
  "name": "restify-rest-client",
  "description": "Restify REST client",
  "version": "3.0.0",
  "main": "lib/index.js",
  "engines": {
    "node": ">=8"
  },
  "author": {
    "name": "Alexey Kucherenko",
    "url": "https://github.com/killmenot"
  },
  "scripts": {
    "prepublishOnly": "npm run build",
    "build": "babel src -d lib",
    "lint": "eslint examples src test",
    "test": "mocha test/* --recursive",
    "coverage": "nyc --reporter=html --reporter=text mocha test/* --recursive",
    "coveralls": "nyc report --reporter=text-lcov | coveralls"
  },
  "nyc": {
    "all": true,
    "include": [
      "src/**/*.js"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/killmenot/rest-client.git"
  },
  "keywords": [
    "restify",
    "rest",
    "client"
  ],
  "bugs": {
    "url": "https://github.com/killmenot/rest-client/issues"
  },
  "license": "MIT",
  "dependencies": {
    "debug": "^4.1.1",
    "hooker": "^0.2.3",
    "restify-clients": "^2.6.6"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "body-parser": "^1.19.0",
    "chai": "^4.2.0",
    "coveralls": "^3.0.3",
    "eslint": "^5.16.0",
    "husky": "^4.3.0",
    "mocha": "^6.1.4",
    "nyc": "^14.1.1",
    "promise-spread": "^0.1.0",
    "restify": "^8.3.2",
    "sinon": "^7.3.2",
    "sinon-chai": "^3.3.0"
  }
}
