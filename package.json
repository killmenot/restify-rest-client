{
  "name": "restify-rest-client",
  "description": "Restify REST client",
  "version": "1.3.1",
  "main": "lib/index.js",
  "engines": {
    "node": ">=4"
  },
  "author": {
    "name": "Alexey Kucherenko",
    "url": "https://github.com/killmenot"
  },
  "scripts": {
    "build": "babel src -d lib",
    "lint": "grunt jshint",
    "test": "mocha test/* --recursive",
    "snyk": "snyk test",
    "coverage": "nyc --reporter=html --reporter=text mocha test/* --recursive",
    "coveralls": "nyc report --reporter=text-lcov | coveralls"
  },
  "nyc": {
    "all": true,
    "include": [
      "lib/**/*.js"
    ],
    "exclude": [
      "test/**/*.js",
      "examples/**/*.js"
    ]
  },
  "pre-commit": [
    "lint",
    "test"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/killmenot/rest-client.git"
  },
  "keywords": [
    "restify",
    "rest",
    "client"
  ],
  "bugs": {
    "url": "https://github.com/killmenot/rest-client/issues"
  },
  "license": "MIT",
  "dependencies": {
    "debug": "^3.1.0",
    "hooker": "^0.2.3",
    "restify-clients": "^2.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "body-parser": "^1.18.2",
    "chai": "^4.1.2",
    "coveralls": "^3.0.0",
    "grunt": "^1.0.2",
    "grunt-cli": "^1.2.0",
    "grunt-contrib-jshint": "^1.1.0",
    "load-grunt-tasks": "^3.5.2",
    "mocha": "^5.0.1",
    "nyc": "^11.4.0",
    "pre-commit": "^1.2.2",
    "promise-spread": "^0.1.0",
    "restify": "^6.3.4",
    "sinon": "^4.4.2",
    "sinon-chai": "^2.14.0",
    "snyk": "^1.69.7"
  }
}
