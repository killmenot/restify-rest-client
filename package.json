{
  "name": "restify-rest-client",
  "description": "Restify REST client",
  "version": "2.0.0",
  "main": "lib/index.js",
  "engines": {
    "node": ">=6"
  },
  "author": {
    "name": "Alexey Kucherenko",
    "url": "https://github.com/killmenot"
  },
  "scripts": {
    "prepublishOnly": "npm run build",
    "build": "babel src -d lib",
    "lint": "eslint examples src test",
    "test": "mocha test/* --recursive",
    "coverage": "nyc --reporter=html --reporter=text mocha test/* --recursive",
    "coveralls": "nyc report --reporter=text-lcov | coveralls"
  },
  "nyc": {
    "all": true,
    "include": [
      "src/**/*.js"
    ]
  },
  "pre-commit": [
    "lint",
    "test"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/killmenot/rest-client.git"
  },
  "keywords": [
    "restify",
    "rest",
    "client"
  ],
  "bugs": {
    "url": "https://github.com/killmenot/rest-client/issues"
  },
  "license": "MIT",
  "dependencies": {
    "debug": "^4.0.1",
    "hooker": "^0.2.3",
    "restify-clients": "^2.5.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.0",
    "@babel/core": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "body-parser": "^1.18.3",
    "chai": "^4.1.2",
    "coveralls": "^3.0.2",
    "eslint": "^5.6.0",
    "mocha": "^5.2.0",
    "nyc": "^13.0.1",
    "pre-commit": "^1.2.2",
    "promise-spread": "^0.1.0",
    "restify": "^7.2.1",
    "sinon": "^6.3.4",
    "sinon-chai": "^3.2.0"
  }
}
